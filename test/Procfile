# Test Procfile - 5 simple processes that print letters with random numbers

alpha: while true; do echo "A $RANDOM"; sleep 1; done
beta: while true; do echo "B $RANDOM"; sleep 1; done
gamma: while true; do echo "C $RANDOM"; sleep 1; done
delta: while true; do echo "D $RANDOM"; sleep 1; done
epsilon: while true; do echo "E $RANDOM"; sleep 1; done
